<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- Repositories -->
  <!-- Linaro Repositories -->
  <remote name="linaro-android" fetch="git://android.git.linaro.org/" review="review.android.git.linaro.org" />
  <remote name="linaro-other" fetch="git://git.linaro.org/" />
  <remote name="git-ti-mirror" fetch="git://git.ti.com/" />
  <remote name="linaro-private" fetch="ssh://git@dev-private-git.linaro.org/linaro-art/" />

  <!-- Remove ART optimization related projects -->
  <remove-project name="platform/art" />
  <remove-project name="platform/build" />
  <remove-project name="platform/external/vixl" />

  <!-- Add ART optimization related projects -->
  <project path="art" name="platform/art" revision="master" remote="linaro-private" />
  <project path="build" name="platform/build" revision="master" remote="linaro-private" >
  <copyfile src="core/root.mk" dest="Makefile" />
  </project>
  <project path="external/vixl" name="platform/external/vixl" revision="master" remote="linaro-private" />

  <!-- Linaro AddOn Projects -->
  <!-- Linaro Tests -->
  <project path="external/busybox" name="platform/external/busybox" revision="linaro-1.21-master-20140515" remote="linaro-android"/>
  <project path="external/waffle" name="platform/external/waffle" revision="linaro-armv8" remote="linaro-android"/>
  <project path="external/piglit" name="platform/external/piglit" revision="linaro-armv8" remote="linaro-android"/>
  <project path="external/powertop" name="platform/external/powertop-2.0" revision="android" groups="common" remote="linaro-android"/>
  <project path="external/powerdebug" name="platform/external/powerdebug" revision="android" groups="common" remote="linaro-android"/>
  <project path="external/ncurses" name="platform/external/ncurses" revision="linaro" groups="common" remote="linaro-android"/>
  <project path="external/pm-qa" name="platform/external/pm-qa" revision="android" groups="common" remote="linaro-android"/>
